    "dayjs": "^1.11.12",
    "flatpickr": "^4.6.13",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.416.0",
    "react": "^18.3.1",
    "react-apexcharts": "^1.4.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "react-icons": "^5.2.1",
    "react-select": "^5.8.0",
    "react-spinners": "^0.14.1",
    "simple-excel-to-json": "^2.3.0",
