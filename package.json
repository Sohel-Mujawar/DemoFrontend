  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint --fix ./src",
    
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "preview": "vite preview",
