    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.10.0",
    "vite": "^5.3.4"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "npm run lint"
    ],
    "*.{ts,tsx}": "eslint --cache --fix"
